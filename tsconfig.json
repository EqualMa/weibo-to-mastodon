{
  "ts-node": {
    "compilerOptions": {
      "target": "ES2017",
      "module": "CommonJS"
    }
  },
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */

    "incremental": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    // "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "strict": true
  },
  "exclude": ["dist"]
}
